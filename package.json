{
  "name": "antigravity-switch",
  "version": "1.0.0",
  "description": "One-click account switcher for Antigravity AI",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "antigravity",
    "quota",
    "ai",
    "switch"
  ],
  "author": "Rishav Mishra <onlyrishavmishra@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.antigravity.switch",
    "productName": "Antigravity Switch",
    "win": {
      "target": "portable",
      "icon": "icon.png"
    },
    "files": [
      "**/*",
      "!dist/*",
      "!brain/*",
      "!.gemini/*"
    ]
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.2.3"
  }
}
